{
  "name": "multi-tenant",
  "private": true,
  "version": "1.0.24",
  "description": "An example of a multi tenant application with Payload",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "_dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:schema": "payload-graphql generate:schema",
    "generate:db-schema": "payload generate:db-schema",
    "generate:types": "payload generate:types",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "seed": "npm run payload migrate:fresh",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start"
  },
  "dependencies": {
    "@nexo-labs/chat-agent": "workspace:*",
    "@nexo-labs/payload-indexer": "workspace:*",
    "@nexo-labs/payload-taxonomies": "workspace:*",
    "@nexo-labs/payload-typesense": "workspace:*",
    "@payloadcms/db-mongodb": "^3.73.0",
    "@payloadcms/db-postgres": "^3.73.0",
    "@payloadcms/next": "^3.73.0",
    "@payloadcms/plugin-import-export": "^3.73.0",
    "@payloadcms/plugin-mcp": "^3.73.0",
    "@payloadcms/plugin-multi-tenant": "^3.73.0",
    "@payloadcms/plugin-nested-docs": "^3.73.0",
    "@payloadcms/richtext-lexical": "^3.73.0",
    "@payloadcms/ui": "^3.73.0",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "graphql": "^16.9.0",
    "lucide-react": "^0.563.0",
    "next": "^15.4.8",
    "next-auth": "^5.0.0-beta.30",
    "payload": "^3.73.0",
    "payload-authjs": "^0.10.0",
    "react": "^19",
    "react-dom": "^19",
    "sharp": "0.32.6",
    "tailwind-merge": "^2.6.0"
  },
  "devDependencies": {
    "@payloadcms/graphql": "^3.73.0",
    "@swc/core": "^1.6.13",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.10.1",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.57.0",
    "eslint-config-next": "15.4.8",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18",
    "tsx": "^4.16.2",
    "tw-animate-css": "^1.4.0",
    "typescript": "5.5.2"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  }
}