{
  "name": "@nexo-labs/chat-agent",
  "version": "1.7.2",
  "description": "Floating chat agent component for React applications with AI integration",
  "keywords": [
    "react",
    "chat",
    "ai",
    "agent",
    "floating",
    "rag",
    "payload-cms"
  ],
  "license": "MIT",
  "author": {
    "name": "Rub√©n Garcia",
    "email": "ruben@nexolabs.xyz",
    "url": "https://nexolabs.xyz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Nexo-Labs/PayloadCMS-Keycloak-MultiTenancy-Base.git",
    "directory": "packages/chat-agent"
  },
  "homepage": "https://github.com/Nexo-Labs/PayloadCMS-Keycloak-MultiTenancy-Base/tree/main/packages/chat-agent#readme",
  "bugs": {
    "url": "https://github.com/Nexo-Labs/PayloadCMS-Keycloak-MultiTenancy-Base/issues"
  },
  "sideEffects": [
    "*.css"
  ],
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/react.ts",
      "import": "./src/react.ts",
      "default": "./src/react.ts"
    },
    "./styles.css": "./src/styles/input.css"
  },
  "main": "./src/react.ts",
  "types": "./src/react.ts",
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsdown && pnpm build:css",
    "build:css": "tailwindcss -i ./src/styles/input.css -o ./dist/styles.css --minify",
    "dev": "tsdown --watch",
    "dev:css": "tailwindcss -i ./src/styles/input.css -o ./dist/styles.css --watch",
    "clean": "rimraf -g {dist,*.tsbuildinfo}",
    "prepublishOnly": "pnpm clean && pnpm build"
  },
  "peerDependencies": {
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "dependencies": {
    "@assistant-ui/react": "^0.11.58",
    "@radix-ui/react-slot": "^1.2.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.0.0",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.24.7",
    "lodash": "^4.17.21",
    "lucide-react": "^0.563.0",
    "react-markdown": "^10.1.0",
    "tailwind-merge": "^2.0.0"
  },
  "devDependencies": {
    "@tailwindcss/cli": "^4.0.0",
    "@types/lodash": "^4.17.20",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "rimraf": "^5.0.10",
    "tailwindcss": "^4.0.0",
    "tsdown": "^0.16.5",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/react.d.mts",
        "import": "./dist/react.mjs",
        "default": "./dist/react.mjs"
      },
      "./server": {
        "types": "./dist/server.d.mts",
        "import": "./dist/server.mjs",
        "default": "./dist/server.mjs"
      },
      "./styles.css": "./dist/styles.css"
    },
    "main": "./dist/react.mjs",
    "types": "./dist/react.d.mts",
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  }
}
