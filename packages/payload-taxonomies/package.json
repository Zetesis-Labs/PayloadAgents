{
  "name": "@nexo-labs/payload-taxonomies",
  "version": "1.7.3",
  "description": "Taxonomies for Payload CMS",
  "keywords": [
    "payload-cms",
    "taxonomies"
  ],
  "license": "MIT",
  "author": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/Nexo-Labs/PayloadCMS-Keycloak-MultiTenancy-Base",
    "directory": "packages/payload-taxonomies"
  },
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "import": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./constants": {
      "types": "./src/constants.ts",
      "import": "./src/constants.ts",
      "default": "./src/constants.ts"
    }
  },
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "pnpm copyfiles && tsdown",
    "dev": "tsdown --watch",
    "clean": "rimraf -g {dist,*.tsbuildinfo}",
    "copyfiles": "copyfiles -u 1 \"src/**/*.{html,css,scss,ttf,woff,woff2,eot,svg,jpg,png,json,mjs}\" dist/ || true",
    "prepare": "pnpm run clean && pnpm run build",
    "taxonomize": "node src/processor/runners/run-taxonomizer.mjs",
    "process": "node src/processor/runners/run-processor.mjs",
    "diagram": "node src/processor/runners/run-diagram-generator.mjs",
    "all": "pnpm run taxonomize && pnpm run process && pnpm run diagram"
  },
  "peerDependencies": {
    "@payloadcms/ui": "^3.72.0",
    "@types/json-schema": "^7.0.11",
    "payload": "^3.72.0",
    "react": "^19.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "copyfiles": "^2.4.1",
    "payload": "^3.72.0",
    "rimraf": "^5.0.10",
    "tsdown": "^0.16.5",
    "typescript": "^5.0.0"
  },
  "publishConfig": {
    "exports": {
      ".": {
        "types": "./dist/index.d.mts",
        "import": "./dist/index.mjs",
        "default": "./dist/index.mjs"
      },
      "./constants": {
        "types": "./dist/constants.d.mts",
        "import": "./dist/constants.mjs",
        "default": "./dist/constants.mjs"
      }
    },
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "dotenv": "16.4.7"
  }
}
